<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Aprobo__c</fullName>
    <description>Indica si el candidato aprobó el test gorilla</description>
    <externalId>false</externalId>
    <formula>IF(Total_Gorilla__c  &gt;= 0.65, true, false)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>¿Aprobó?</label>
    <type>Checkbox</type>
</CustomField>
